{
  "family": "webserver",
  "containerDefinitions": [
    {
      "name": "webserver",
      "image": "nginx:1.24-alpine",
      "portMappings": [
        {
          "name": "htttp",
          "containerPort": 80
        }
      ],
      "memory": "100",
      "cpu": "99"
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256"
}
